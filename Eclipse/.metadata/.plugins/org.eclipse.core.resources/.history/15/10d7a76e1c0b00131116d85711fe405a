package Aula_16_08_13_Figura;

public class Principal {

	public static void main(String[] args) {
		
		Quadrado newQuadrado = new Quadrado(2, 3, 5);
		Circulo newCirculo = new Circulo(4, 5, 7);
		Generalizadora newGeneral = new Generalizadora(5, 6);
		
		newGeneral.setCor("branco");
		newQuadrado.desenha();
		newCirculo.desenha();
		newGeneral.desenha();
		newQuadrado.setCor("Azul");
		newCirculo.setCor("verde");
		
	}

}
