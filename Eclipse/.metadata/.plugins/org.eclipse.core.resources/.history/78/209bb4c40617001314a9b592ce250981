package Janela;

import javax.swing.*;

public class Janela {

	public static void main(String[] args) {
		int i = 0;
		JFrame frame = new JFrame("aplicacao java");
		JButton but = new JButton("clic");
		JLabel texto = new JLabel("quantidade de clics");
		JPanel panel = new JPanel();

		frame.add(panel);
		panel.add(but);
		for (int j = 0; j < 10; j++) {
			panel.add(texto,j);	
		}
		
		frame.setVisible(true);

	}

}
