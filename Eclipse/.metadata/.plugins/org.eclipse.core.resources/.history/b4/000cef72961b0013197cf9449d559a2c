package Exercicio;

import java.awt.BorderLayout;

import javax.swing.BoxLayout;
import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.JButton;
import javax.swing.JTextField;
import javax.swing.JLabel;

public class Tela extends JFrame {

	public Tela() {
		super("Tela Interação");
		JPanel painel1 = new JPanel();
		JPanel painel2 = new JPanel();
		JPanel painel3 = new JPanel();
		JPanel painel4 = new JPanel();
		JTextField texto = new JTextField(25);

		JLabel novo = new JLabel("informe alguma coisa ");

		JButton butaoOeste1 = new JButton("norte 1");
		JButton butaoOeste2 = new JButton("norte 2");
		JButton butaoOeste3 = new JButton("norte 3");
		JButton butaoOeste4 = new JButton("norte 4");
		JButton butaoOeste5 = new JButton("norte 5");
		JButton butaoLeste = new JButton("leste");
		JButton butaoOeste = new JButton("oeste");

		add(painel1, BorderLayout.WEST);
		add(painel2, BorderLayout.NORTH);
		painel1.add(butaoOeste1, BoxLayout.Y_AXIS);
		painel1.add(butaoOeste2, BoxLayout.Y_AXIS);
		painel1.add(butaoOeste3, BoxLayout.Y_AXIS);
		painel1.add(butaoOeste4, BoxLayout.Y_AXIS);
		painel1.add(butaoOeste5, BoxLayout.Y_AXIS);
		painel2.add(novo);
		painel2.add(texto);
		setSize(300, 300);
		setVisible(true);

	}

	public static void main(String[] args) {
		new Tela();

	}

}
