package Exercicio;
import java.awt.BorderLayout;

import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.JButton;
import javax.swing.JTextField;
import javax.swing.JLabel;
import javax.swing.LayoutStyle;
public class Tela extends JFrame {

	
public Tela(){
	super("Tela Interação");
JPanel painel1 = new JPanel();
JPanel painel2 = new JPanel();
JPanel painel3 = new JPanel();
JPanel painel4 = new JPanel();
JTextField texto = new JTextField(25);
LayoutStyle botaoLeste = new LayoutStyle();
JLabel novo = new JLabel("informe alguma coisa ");


JButton butao1 = new JButton("norte 1");
JButton butao2 = new JButton("norte 2");
JButton butao3 = new JButton("norte 3");
JButton butao4 = new JButton("norte 4");
JButton butao5 = new JButton("norte 5");
add(painel1, BorderLayout.NORTH);
add(painel2, BorderLayout.SOUTH);
painel1.add(butao1);
painel1.add(butao2);
painel1.add(butao3);
painel1.add(butao4);
painel1.add(butao5);
painel2.add(novo );
painel2.add(texto);

setSize(300,300);
setVisible(true);

	
}
	
	
	public static void main(String[] args) {
		new Tela();
		

	}

}
