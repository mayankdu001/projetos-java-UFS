
package geometria;


public class Area {
    
    public static double AreaCalc(double base){
        double area;
        area = base * base;
        return area;
    }
    
    public static double AreaCalc(double base, double altura){
        double area;
        area = base * altura;
        return area;
    }
    
    public static double AreaCalc(double baseMenor, double baseMaior, double altura){
        double area;
        area = (baseMenor + baseMaior)* altura/2;
        return area;
    }

}
