package Lista;

class Node {
	Object data;
	Node nextNode;

	public Node(Object elemento, Node node) {
		this.data = elemento;
		this.nextNode = node;
	}

	public Object getObject() {
		return data;
	}

	public Node getNext() {
		return nextNode;
	}

}

public class ListNode {
	Node firstNode;
	Node lastNode;
	String nome;

	public ListNode() {
		this("nome");
	}

	public ListNode(String nome) {
		this.nome = nome;
		firstNode = lastNode = null;

	}

}
